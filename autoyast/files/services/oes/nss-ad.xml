<?xml version="1.0"?>
<!DOCTYPE profile>
<profile xmlns="http://www.suse.com/1.0/yast2ns" xmlns:config="http://www.suse.com/1.0/configns">
  <novell-nssad>
    <domain_name>%%AD_DOMAIN_NAME%%</domain_name>
    <admin_name>%%AD_USER_NAME%%</admin_name>
<!--
    <admin_password>Replace this text with the real password</admin_password>
-->
    <domain_admin_group>%%AD_DOMAIN_ADMIN_GROUP%%</domain_admin_group>
    <ad_context>%%AD_COMPUTER_CONTAINER%%</ad_context>
    <pre_created_object>%%AD_USE_PRECREATED_COMPUTEROBJ%%</pre_created_object>
    <ad_uid_generate_mode>%%AD_CREATE_NIT_UIDS%%</ad_uid_generate_mode>
    <nit_start_range config:type="integer">%%AD_NIT_UID_START%%</nit_start_range>
    <nit_end_range config:type="integer">%%AD_NIT_UID_END%%</nit_end_range>
  </novell-nssad>
</profile>
