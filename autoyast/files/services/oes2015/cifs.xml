<?xml version="1.0"?>
<!DOCTYPE profile>
<profile xmlns="http://www.suse.com/1.0/yast2ns" xmlns:config="http://www.suse.com/1.0/configns">
  <novell-cifs>
    <cifs_edir_contexts config:type="list">
     <listentry>%%CIFS_SEARCH_BASE%%</listentry>
    </cifs_edir_contexts>
    <create_new_user>no</create_new_user>
    <ldap_server>%%LDAP_1%%</ldap_server>
    <cifs_ldap_port config:type="integer">636</cifs_ldap_port>
    <proxy_user>cn=OESCommonProxy_%%HOST_NAME%%,%%SERVER_CONTEXT%%</proxy_user>
<!--
    <proxy_user_password>Replace this text with the real password</proxy_user_password>
-->
    <server_context>%%SERVER_CONTEXT%%</server_context>
    <use_casa_for_credentials>yes</use_casa_for_credentials>
    <use_secure_port>yes</use_secure_port>
    <subtree_search>yes</subtree_search>
    <usercontext_rights>yes</usercontext_rights>
  </novell-cifs>
</profile>
