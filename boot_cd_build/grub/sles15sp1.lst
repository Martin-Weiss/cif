color white/blue black/light-gray
default 0
timeout 30
root (cd)
gfxmenu /grub/message

title back
        configfile /grub/instserver.lst

title SLES15SP1
	kernel /kernel/sles15sp1/linux info=http://10.1.1.1/autoyast/info-sles15sp1.txt netsetup=hostip,gateway,nameserver netmask=255.255.255.0 gateway=10.1.1.1 nameserver=10.1.1.1 netwait=10 
	initrd=/kernel/sles15sp1/initrd

title SLES15SP1 manual installation
	kernel /kernel/sles15sp1/linux info=http://10.1.1.1/autoyast/info-sles15sp1-manual.txt netsetup=hostip,gateway,nameserver netmask=255.255.255.0 gateway=10.1.1.1 nameserver=10.1.1.1 netwait=10 
	initrd=/kernel/sles15sp1/initrd

title SLES15SP1 rescue mode
	kernel /kernel/sles15sp1/linux info=http://10.1.1.1/autoyast/info-sles15sp1-rescue.txt netsetup=hostip,gateway,nameserver netmask=255.255.255.0 gateway=10.1.1.1 nameserver=10.1.1.1 netwait=10 
	initrd=/kernel/sles15sp1/initrd

